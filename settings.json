{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": [
      "Bash(gh *)",
      "Bash(git *)",
      "Bash(sleep *)",
      "Bash(npm *)",
      "Bash(npx *)",
      "Bash(yarn *)",
      "Bash(make *)",
      "Bash(docker *)",
      "Bash(aws * get*)",
      "Bash(aws * list*)",
      "Bash(aws * describe*)",
      "Bash(aws * head*)",
      "Bash(aws * search*)",
      "Bash(aws * batch-get*)",
      "Bash(aws * scan*)",
      "Bash(aws * query*)",
      "WebSearch",
      "WebFetch",
      "Bash(npx prettier --write \"**/*\")"
    ],
    "ask": ["Bash(git push *)"]
  },
  "model": "opus",
  "enabledPlugins": {
    "gopls-lsp@claude-plugins-official": true,
    "frontend-design@claude-plugins-official": true
  }
}
